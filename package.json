{
  "name": "pdf-merger-js",
  "version": "3.2.0",
  "description": "merge multible PDF documents, or parts of them, to a new PDF document",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "standard": "standard",
    "standard:fix": "standard --fix",
    "test": "jest --testPathIgnorePatterns 'performance.test.js' --detectOpenHandles",
    "test:watch": "jest --testPathIgnorePatterns 'performance.test.js' --watch --forceExit --detectOpenHandles",
    "test:performance": "node --expose-gc ./node_modules/.bin/jest --runInBand --detectOpenHandles performance.test.js"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": "github:nbesli/pdf-merger-js",
  "keywords": [
    "pdf",
    "merge"
  ],
  "author": "nbesli",
  "license": "MIT",
  "dependencies": {
    "pdfjs": "^2.4.3"
  },
  "devDependencies": {
    "fs-extra": "^9.1.0",
    "jest": "^26.6.3",
    "pdf-diff": "^0.1.1",
    "standard": "^16.0.3"
  }
}
